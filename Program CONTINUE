#include <iostream>
using namespace std;
// TUGAS PRAKTIKUM DIRUMAH (CONTINUE) 07/04/2020 MUHAMMAD REZEKI
int main (){
	/* YANG AKAN DITAMPILKAN ADALAH ANGKA GANJIL 
	DAN ANGKA GENAP DARI RANGE 1-100 AKAN DILEWATI OLEH STATEMENT CONTINUE
	*/ 
	for ( int i = 1 ; i <= 100 ; i++){
	if ( i % 2 == 0 ) continue;
	cout << i <<" angka genap dilewati" << endl;
	}

return 0;
}
